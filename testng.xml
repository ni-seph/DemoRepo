<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
<listeners>
	<listener class-name="Academy.listeners"/>
	<listener class-name="resources.ExtentReporterNG"/>
</listeners>
  <test thread-count="5" name="AppTest">
    <classes>
      <class name="Academy.AppTest"/>
    </classes>
    </test>
    <test name="validating title">
    <classes>
          <class name="Academy.ValidateTitle"/>
    </classes>
    </test>  
    <test name="hometest">
    <classes>
          <class name="Academy.HomePage"/>
    
    </classes>
    </test>
    <test name="validating navbar">
    <classes>
      <class name="Academy.ValidateNavBar"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
